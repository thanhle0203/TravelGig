$(document).ready(function() {
	var searchText = $("searchLocation").val();
	$('#tblHotel tr').not(':first').remove();
	$.get("http://localhost:8282/searchHotel/"+searchText, {
		
	}, function(responseText) {
		$.each(responseText, function(key1, value1) {
			$("#tblHotel").append("<tr><td>" + value1.hotelName + "</tr><td><img class='hotelImage' src='"+value1.imageURL+"'width='300' height='300'/> </td><><td>" + value1.averagePrice+ "</td><td>" + value1.starRating + "</td></tr>")
		});
	});
});