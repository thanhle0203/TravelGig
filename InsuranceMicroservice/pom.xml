		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.thanhle</groupId>
	<artifactId>InsuranceMicroservice</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>InsuranceMicroservice</name>
